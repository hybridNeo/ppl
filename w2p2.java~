import java.util.*;
public class w2p2{
	static int a =100;
	static class StaticNested{
				public void footer(){
			System.out.println(a);
			}
	}
	class NestedClass{
			public String a = "Hello";
		
			public void foo(){
				System.out.println("Booo\n");
			}
		}
		
	public static void main(String[] args){
	  
		w2p2.NestedClass n = new w2p2.NestedClass();
		n.foo();
		StaticNested s = new StaticNested();
		s.footer();	
	}
	
}
